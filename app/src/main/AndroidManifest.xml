<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dansiapa.mvvmkotlin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/breaking_news"
        android:label="@string/app_name"
        android:roundIcon="@drawable/breaking_news"
        android:supportsRtl="true"
        android:theme="@style/Theme.MvvmKotlin"
        android:usesCleartextTraffic="true">
        <activity android:name=".view.RegisterActivity"></activity>
        <activity android:name=".view.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.HomeActivity" />
        <activity android:name=".view.MainActivity"></activity>
    </application>

</manifest>